        {% endblock %}

        <script type="text/javascript">
        	//Script pour le d√©filement de l'agenda
            $(document).ready(function() 
			{				
				$("#defilagenda").als({
					visible_items: 2,
					scrolling_items: 1,
					orientation: "vertical",
					circular: "yes",
					autoscroll: "yes",
					start_from: 1
				});
			});
        </script>

</head>
    <body>
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    {% include '_carousel.html.twig' %}
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                   {% include '_menu.html.twig' %}
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    &nbsp;
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    <div>
                        <h6>Agenda</h6>
                        {% block agenda %}{% endblock %}
                    </div>
                </div>
                <div class="col-md-9">
                    {% block body %}{% endblock %}
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    {% include '_footer.html.twig' %}
                </div>
            </div>
            


            
